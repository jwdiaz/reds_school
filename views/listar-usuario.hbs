
{{>header }}

    {{> menu-docente}}
    <h2>Listar Aspirantes</h2>
    <hr/>

    <div class="row">
        <div class="col-md-12 mx-auto">
            <table class="table">
                <thead>
                <tr>
                    <th scope="col">Documento</th>
                    <th scope="col">Nombre</th>
                    <th scope="col">Telefono</th>
                    <th scope="col">Correo</th>
                    <th scope="col">Perfil</th>
                </tr>
                </thead>
                <tbody>
                {{#each usuarios}}
                    <tr>
                        
                        <td>{{ documento }}</td>
                        <td>{{ nombre }}</td>                   
                        <td>{{ correo }}</td>
                        <td>{{ telefono }}</td>
                         <td>{{ tipo }}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
    </div>
    {{>footer }}